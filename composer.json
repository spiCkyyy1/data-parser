{
  "name": "hassam/data-parser",
  "description": "Data Parser for Log Files",
  "type": "project",
  "require": {
    "php": ">=8.4",
    "symfony/console": "^7.0",
    "symfony/finder": "^7.0",
    "league/csv": "^9.16",
    "psr/log": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}